% correction type
type:: digitizer

% description of the digitizer corrections setup
name:: Soundblaster VBOX test

% names of the channels as they appear in the digitizer identification
#startmatrix:: channel identifiers
	Primarni ovladac zvuku, channel 1;
  Primarni ovladac zvuku, channel 2;
#endmatrix:: channel identifiers

% links to the channel corrections of particular channels
#startmatrix:: channel correction paths
	channel\sb1_channel.info	
#endmatrix:: channel correction paths


#startsection:: interchannel timeshift

  % 2D array with the list of values of the correction
  %  rows: vectors of correction values, eg. relative timeshifts to the first channel
  %  columns: primary parameter
  #startmatrix:: interchannel timeshift
    0.0; 0.0;
  #endmatrix:: interchannel timeshift

  % type of the value of the correction 
  %  vector: array of row vectors of real or complex values (only primary parameter supported)
  format:: vector

#endsection:: interchannel timeshift